{
  "name": "dag-jose",
  "version": "4.0.0",
  "description": "Typescript implementation of the IPLD dag-jose format",
  "main": "./lib/index.js",
  "type": "module",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
    "build": "./node_modules/.bin/tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean",
    "lint": "./node_modules/.bin/eslint ./src --ext .js,.ts",
    "clean": "rm -rf ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceramicnetwork/js-dag-jose.git"
  },
  "keywords": [
    "ipld",
    "ipfs",
    "jose",
    "dag"
  ],
  "author": "Joel Thorstensson <oed@3box.io>",
  "license": "(Apache-2.0 OR MIT)",
  "bugs": {
    "url": "https://github.com/ceramicnetwork/js-dag-jose/issues"
  },
  "homepage": "https://github.com/ceramicnetwork/js-dag-jose#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@stablelib/x25519": "^1.0.1",
    "@types/jest": "^29.1.2",
    "@types/lodash.transform": "^4.6.6",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-jest": "^29.2.0",
    "blockcodec-to-ipld-format": "^2.0.0",
    "did-jwt": "^6.0.0",
    "eslint": "^8.12.0",
    "eslint-config-3box": "^0.4.1",
    "eslint-plugin-jest": "^27.1.2",
    "fast-json-stable-stringify": "^2.1.0",
    "ipld": "^0.30.0",
    "ipld-in-memory": "^8.0.0",
    "jest": "^29.2.0",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2",
    "uint8arrays": "^4.0.2"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^9.0.0",
    "multiformats": "^11.0.0"
  }
}
